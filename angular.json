{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "newProjectRoot": "projects",
  "projects": {
    "cph-app": {
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ],
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "budgets": [
                {
                  "maximumError": "1mb",
                  "maximumWarning": "500kb",
                  "type": "initial"
                },
                {
                  "maximumError": "4kb",
                  "maximumWarning": "2kb",
                  "type": "anyComponentStyle"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "browser": "src/main.ts",
            "index": "src/index.html",
            "outputPath": "dist/cph-app",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "preserveSymlinks": true,
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src",
                "src/scss"
              ],
              "sass": {
                "silenceDeprecations": [
                  "color-functions",
                  "global-builtin",
                  "import",
                  "mixed-decls"
                ]
              }
            },
            "styles": [
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "cph-app:build:development"
            },
            "production": {
              "buildTarget": "cph-app:build:production"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 1420
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    }
  },
  "version": 1
}